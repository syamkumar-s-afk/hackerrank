  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@monaco-editor/react": "^4.6.0",
    "lucide-react": "^0.575.0",
    "next": "14.2.14",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.0-beta.8",
    "tailwindcss": "^4.0.0-beta.8"
  }
}
